{
  "name": "openapi-to-msw",
  "version": "1.0.0",
  "description": "Generate random mock data from OpenAPI descriptions for msw.",
  "main": "dist/cli.js",
  "module": "./dist/cli.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "require": "./dist/cli/index.js",
      "import": "./dist/cli/index.mjs",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "bin": {
    "openapi-to-msw": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "fmt": "prettier {example,src}/**/*.{js,jsx,ts,tsx,css,md,html} --write",
    "test": "vitest run",
    "release": "semantic-release",
    "clean": "rimraf dist",
    "prepare": "run-s clean build"
  },
  "author": "schiughi<schiughi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "cac": "^6.7.12",
    "msw": "^0.36.3",
    "oazapfts": "^3.5.0",
    "prettier": "2.5.1",
    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.5",
    "@types/prettier": "^2.4.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^10.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "vitest": "^0.0.132"
  },
  "repository": "git+https://github.com/schiughi/openapi-to-msw.git",
  "bugs": {
    "url": "https://github.com/schiughi/openapi-to-msw/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "msw",
    "mock",
    "service-worker",
    "openapi"
  ]
}
